{
    "name": "Kenny Logins",
    "version": "1.1",
    "permissions": [
        "tabs","<all_urls>"
    ],
    "content_scripts": [
        {
        "matches": [
            "http://*/*",
            "https://*/*"
            ],
        "js": ["content.js"],
        "run_at": "document_end"         // pay attention to this line
        }
    ], 
    "manifest_version":2
}